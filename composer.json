  "homepage": "https://github.com/mhannan-dev/sparktro-installer",
  "keywords": [
    "laravel",
    "installer",
    "setup",
    "sparktro"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "v9.2"
}
