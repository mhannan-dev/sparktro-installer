{
  "name": "sparktro/ignite",
  "description": "Sparktro Laravel App Installer Package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Hannan",
      "email": "mdhannan.info@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sparktro\\Ignite\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0"
  },
  "extra": {
